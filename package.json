{
  "name": "notas-vue-fire",
  "version": "0.1.0",
  "description": "Frontend de una apliación de Notas en VueJS",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --production",
    "lint": "vue-cli-service lint",
    "clean": "rm -rf node_modules && npm run undist",
    "reinstall": "npm run clean && npm run undist && npm install",
    "undist": "rm -rf dist",
    "start": "node server.js"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/joseluisgs/notas-vue-nem"
  },
  "homepage": "",
  "author": {
    "name": "José Luis González Sánchez",
    "twitter": "@joseluisgonsan"
  },
  "bugs": {
    "url": "https://github.com/joseluisgs/notas-vue-nem/issues",
    "twitter": "@joseluisgonsan"
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "bootstrap-vue": "^2.15.0",
    "connect-history-api-fallback": "^1.6.0",
    "core-js": "^3.6.5",
    "express": "^4.18.2",
    "firebase": "^7.15.1",
    "moment": "^2.26.0",
    "npm": "^6.14.5",
    "serve-static": "^1.14.1",
    "vue": "^2.6.11",
    "vue-moment": "^4.1.0",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-router": "~4.4.0",
    "@vue/cli-plugin-vuex": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "lint-staged": "^9.5.0",
    "vue-template-compiler": "^2.6.11"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
